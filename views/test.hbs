<div class="container">
    <div class="card m-3 p-4">
        <div class="card-title"><h3>Please start the test</h3></div>
        <div class="card-body">
            {{#each test}}
                <div><p class="font-weight-bold">{{this.question}}<p></div>
                {{#each this.options}}
                    <div class="custom-control custom-radio">
                        <input type="radio" id="customRadio1" name="{{this}}" class="custom-control-input">
                        <label class="custom-control-label" for="customRadio1">{{this}}</label>
                    </div>
                {{/each}}
                <br>
            {{/each}}
        </div>
    </div>
</div>